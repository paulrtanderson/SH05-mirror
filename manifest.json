{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A React Chrome Extension",
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "unlimitedStorage",
      "notifications",
      "alarms",
      "tabs",
      "sidePanel"
    ],
    "background": {
      "service_worker": "dist/background.bundle.js"
    },
    "side_panel": {
      "default_path": "index.html"
    },
    "action": {
      "default_title": "Open Side Panel"
    }

}